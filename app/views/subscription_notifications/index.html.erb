<p id="notice"><%= notice %></p>

<h1>Subscription Notifications</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @subscription_notifications&.each do |subscription_notification| %>
      <tr>
        <td><%= link_to 'Show', subscription_notification %></td>
        <td><%= link_to 'Edit', edit_subscription_notification_path(subscription_notification) %></td>
        <td><%= link_to 'Destroy', subscription_notification, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Subscription Notification', new_subscription_notification_path %>
